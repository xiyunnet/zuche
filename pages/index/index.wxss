
.main{position: fixed;width: 100%;padding:10px;border-radius: 10px;height: 100vh;left: 0;top: 0;overflow: hidden;}
cover-view{box-sizing: border-box;}
map{width: 100%;height: 100%;left: 0;top: 0;float: left;border-radius: 10px;z-index: 100;}
page{background: #fff;}
.main .topleft{position: absolute;width:25px;height:25px;z-index: 100;top:10px;left:10px;}
.main .topright{position: absolute;width:25px;height:25px;z-index: 100;top:10px;right:10px;}
.main .bottomleft{position: absolute;width:25px;height:25px;z-index: 100;bottom:10px;left:10px;}
.main .bottomright{position: absolute;width:25px;height:25px;z-index: 100;bottom:10px;right:10px;}

.is_select_point{position: absolute;width:14px;height:14px;border-radius:50%;top: calc(50% - 10px);left:calc(50% - 10px);display:flex;justify-items: center;justify-content: center;}

@keyframes point{
  0% {transform: scale(1);opacity:0;}
  20% {transform: scale(1.15);opacity:0.05;}
  40% {transform: scale(1.3);opacity:0.1;}
  60% {transform: scale(1.45);opacity:0.2;}
  80% {transform: scale(1.5);opacity:0.3;}
  100% {transform: scale(1);opacity:0;}
}


.jt_box{position: absolute;width: 100%;height: 100%;left: 0;top: 0;}
.is_select_point .show_border{position: absolute;width:20px;height:20px;background: rgba(255,255,255,.8);border-radius: 50%;top: -3px;left: -3px;z-index: -1px;}
.is_select_point .in_box{position: absolute;width: 100%;height: 100%;background: #0084ec;border-radius: 50%;left: 0;top: 0;z-index: 100;}

.add_info{position: absolute;width:500rpx;background: #fff;bottom:25px;padding: 10px;border-radius:25px;z-index: 100;left:-280rpx;box-shadow: 1px 1px 5px 0 #ccc,inset 1px 1px 1px rgba(255,255,255, .44);padding-left: 15px;padding-right: 15px;}
.add_info .title{float: left;width: 100%;line-height:20px;font-weight:bold;font-size: 17px;}
.add_info .addr{float: left;width: 100%;line-height: 16px;font-size: 13px;color: #999;}
.add_info .bt_box{float: left;width: 100%;margin-top:10px;}
.add_info .bt_box view{float: right;height:32px;line-height: 30px;background: #0084ec;color: #fff;padding-left: 20px;padding-right: 20px;border-radius:18px;font-size: 13px;border: 1px solid #0084ec;margin-left: 10px;}
.add_info .bt_box .bt{border: 1px solid #0084ec;background: #fff;color: #0084ec;}
.add_info .icon-xiangxia{position: absolute;color: #fff;bottom:-9px;left:278rpx;}
.ss{box-shadow: 1px 1px 1px rgba(0,0,0, .15), inset 1px 1px 1px rgba(255,255,255, .44);}

.nav{position: fixed;width: 100%;z-index: 10000;left: 0;top: 0;text-align: center;}
.nav .area{float: left;width: 100%;height: 44px;line-height: 44px;display: flex;justify-self:center;justify-content: center;z-index: 100;}
.nav .area .icon{margin-left: 3px;margin-right: 3px;font-size: 22px;}
.nav .area  text{font-size: 18px;}
.nav .logo{position: absolute;width: 50px;height: 50px;border-radius: 50%;background: #efefef;line-height: 50px;left:20px;top:20px;font-size: 30px !important;color: #666;z-index: 100;}
.nav .area .icon-chengshi{line-height:45px;}
.nav .area .icon-chevron-down{line-height: 50px;}
.nav .area picker{position: absolute;width:30vw;height: 44px;background: #ccc;z-index: 100;opacity: 0;z-index: 1000;}


.my_start{float: left;width: 100%;height:20px;line-height: 20px;padding-left:25px;margin-bottom: 10px;overflow: hidden;}
.my_start .p{position: absolute;width: 20px;color: #0084ec;font-size:15px !important;left: 0;top: 0;height: 20px;line-height:23px;text-align: center;opacity: .8;}
.my_start view{float: left;width:100%;}
.my_start .addr{color: #0084ec;padding-left:3px;padding-right: 3px;font-weight: bold;max-width:60%;overflow: hidden;}
.my_start .addr text{float: left;height: 20px;line-height: 20px;}

.my_point{position: absolute;width:40px;height:40px;overflow: visible !important;}
.my_point .item{background: #06cf3a;width:20px;height:20px;border-radius: 50%;position: absolute;z-index:0;animation:point 2s linear infinite ;top:10px;left: 10px;position: absolute;z-index: 10;}
.my_point .point{position: absolute;width:20px;height:20px;top:10px;left:10px;z-index: 100;}
.cover_main{z-index: 100000;position: fixed;}
.start_end{width:420rpx;z-index: 100;height:200rpx;font-size: 13px;}
.start_end .in_box{float: left;width:100%;height:40px;background: #fff;border-radius:40px;padding-left:140rpx;overflow: hidden;box-shadow: 1px 1px 5px 0 #ccc,inset 1px 1px 1px rgba(255,255,255, .44);z-index: 10000;}
.start_end .left{position: absolute;width: 130rpx;left: 0;z-index: 100;top: 0;height: 40px;text-align: center;padding-left: 10rpx;}
.start_end .img{width:50rpx;left:200rpx;position: absolute;height: 100rpx;top:45px;z-index: 100;}
.start_end .left .time{float: left;width: 100%;height:18px;line-height:18px;margin-top:2px;font-weight: bold;z-index: 100;text-overflow: ellipsis;}
.start_end .left  .c{float: left;width: 100%;height:16px;line-height:16px;color: #999;font-size: 11px;margin-top: -1px;}
.start_end .right{float: left;width: 100%;height: 40px;padding-right:20px;}
.start_end .title{float: left;width: 100%;height: 18px;line-height: 18px;font-weight: bold;overflow: hidden;text-overflow: ellipsis;font-size: 14px;margin-top:3px;}
.start_end .addr{float: left;width:100%;height:16px;line-height:16px;font-size: 12px;overflow: hidden;text-overflow: ellipsis;margin-top: -1px;}
.start_end .right .icon{position: absolute;width:16px;right: 0;height:16px;font-size: 20px;z-index: 100;top: 11px;right: 5px;}
.blue_bg{background: rgb(0, 132, 236,.3);color: #0084ec;}
.yellow_bg{background: rgb(255, 107, 1,.3);color: #ff6b01;}
.start_end button{position: absolute;z-index: 100000;height: 100px;}


.marker_item{float: left;width: 100%;max-height:40vh;overflow: hidden;padding-bottom: 10px;overflow: auto;height: 0;}
.marker_item .item{float: left;width: 100%;margin-top: 10px;padding-right:50px;}
.marker_item .title{float: left;width: 100%;height: 24px;line-height: 24px;overflow:hidden;text-overflow: ellipsis;font-weight: bold;font-size: 15px;}
.marker_item  .addr{float: left;width: 100%;line-height: 16px;font-size: 12px;color: #999;}
.marker_item::-webkit-scrollbar{display: none;width: 0;}
.marker_item .bt{position: absolute;width: 50px;height: 28px;line-height: 28px;background: #0084ec;color: #fff;font-size: 13px;text-align: center;border-radius: 15px;right: 0;top: 0;z-index: 100;}



.dh_icon{float: left !important;top:50px;}
.dh_icon_show{top: 0;}

page-container .in_box{float: left;width: 100%;height:100%;background: #fff;border-top-left-radius: 25px;border-top-right-radius: 25px;min-height:60vh;padding-top: 30px;padding-bottom:10px;}
page-container .in_box::-webkit-scrollbar{display: none;width: 0;}
page-container .in_box .handle{position: absolute;width: 100%;text-align: center;height: 30px;line-height: 30px;top: 0;left: 0;}
page-container .in_box .handle text{position: absolute;width:150rpx;height:6px;background: #efefef;border-radius: 4px;top:10px;left: 300rpx;}
page-container .in_box .handle icon{float:right;font-size: 22px;margin-right: 20px;margin-top: 5px;z-index: 100;color: #666;}
.page_main{float: left;width: 100%;overflow: hidden;min-height: 50vh;padding-left: 10px;padding-right: 10px;overflow-y: auto;padding-top: 85px;padding-bottom:30px;}
page-container .page_main::-webkit-scrollbar{display: none;width: 0;}


.d_state{width: 100%;height: 24px;line-height: 24px;padding-left: 30px;color: #999;overflow: hidden;text-overflow: ellipsis;position: absolute;top:55px;left: 0;font-size: 14px;}
.d_state .yuan{position: absolute;width: 24px;height:24px;border-radius: 50%;left: 0;top: 0px;background:green;color: #fff;line-height: 24px;text-align: center;font-size: 12px;}
.d_state text{color: #0084ec;font-weight: bold;max-width: 70%;overflow: hidden;padding-left: 3px;padding-right: 3px;}
.step_item{float: left;width: 100%;min-height:20px;margin-top: 10px;padding-left: 30px;}
.step_item .d_jt{position: absolute;width:30px;height: 20px;font-size: 30px;left:-5px;top: 0;font-weight: bold;text-align: center;}
.step_item .road_name{float: left;width: 100%;font-size: 16px;font-weight: bold;}
.step_item .dis{float: left;width: 100%;font-size: 12px;color: #999;margin-top: 2px;}
.step_item .info{float: left;width: 100%;margin-top:3px;line-height: 16px;font-size: 14px;}
.step_list{float: left;width: 100%;height: 100%;overflow:auto;}
.step_list::-webkit-scrollbar{display: none;width: 0;}
.red_bg{background: crimson !important;}
.icon-Connectorup-px,.icon-Connectorup-px1,.icon-ConnectorUturndown-px,.icon-ConnectorUturndown-px1{font-size: 26px !important;}
.icon-Connectorup-px{font-size: 24px !important;}
.icon-checkbox-blank-circl{font-size: 24px !important;}
.how_to_go{position: absolute;width: 100%;height: 40px;top:2px;left: 0;}
.how_to_go .item{float: left;margin-left:10px;border-radius: 5px;border:2px solid #efefef;padding-bottom:3px;width:100rpx;}
.how_to_go .item view{float: left;width:100%;height: 24px;line-height: 24px;text-align: center;color: #666;font-size:22px;}
.how_to_go .item text{float: left;width:100%;height: 16px;line-height: 16px;color: #999;text-align: center;font-size:10px;}
.how_to_go .select{border: 2px solid #0084ec;background: #0084ec;color: #fff !important;}
.how_to_go .select view{color: #fff;}
.how_to_go .select text{color: #fff;}

.show{display: block !important;}
.hide{display: none !important;}

.shop_info{width:300rpx;height:40px;background: #fff;border-radius:20px;padding-left: 20px;padding-right: 20px;}
.shop_info .name{float: left;width: 100%;height:18px;line-height: 18px;font-weight: bold;overflow: hidden;text-overflow: ellipsis;font-size: 14px;margin-top:3px;}
.shop_info .shop_addr{float: left;width: 100%;height:16px;line-height: 16px;overflow: hidden;text-overflow: ellipsis;font-size: 12px;color: #999;}
.is_select_shop{background: #0084ec;color: #fff;}
.is_select_shop .shop_addr{color: #fff !important;}

.nav_bottom{position: fixed;width: 100%;left: 0;z-index: 1000;bottom: 10vh;}
.go_bt{float: left;width: 100%;display: flex;justify-content: center;}
.go_bt view{float: left;width:200rpx;height:200rpx;background: #0084ec;border-radius: 50%;line-height: 200rpx;text-align: center;font-size: 20px;color: #fff;}
.go_bt view::before{content: '';width: 220rpx;height:220rpx;background: rgba(0, 132, 236,.5);left:-10rpx;top: -10rpx;position:absolute;border-radius: 50%;}
.go_bt view::after{content: '';width: 200rpx;height: 200rpx;left:-0rpx;top: -0rpx;position:absolute;border-radius: 50%;box-shadow: rgba(0, 132, 236,.4) 0 0 3px 15px;}
.go_bt view text{float: left;width: 100%;z-index: 100;font-size:28px;}

.nav_bottom .bts{position: absolute;right:20px;width: 40px;bottom:-4vh;}
.nav_bottom .bts view{float: right;width:50px;height: 50px;background: #efefef;border-radius: 50%;text-align: center;margin-top: 10px;line-height: 50px;font-size: 24px !important;}
.nav_bottom button{position: absolute;width: 100%;height: 100%;top: 0;left: 0;z-index: 100;opacity: 0;}
.get_my_position_show{display: block !important;opacity: 1 !important;}


.search_box{position: fixed;width: 100%;z-index: 100000;padding-left: 50rpx;padding-right: 50rpx;}
.search_box .in_box{float: left;width: 100%;}
.search_box input{float: left;width: 100%;height:55px;line-height: 55px;box-sizing: border-box;padding-left: 15px;padding-right: 15px;background: rgba(255,255,255, .5);padding-right: 50px;border-radius: 5px;font-size: 17px;}
.search_box .icon{position: absolute;width:50px;height: 55px;font-size:24px;line-height: 55px;text-align: center;right: 0;z-index: 100;color: #0084ec;}
.car_num{float: left;width: 100%;color: #0084ec;font-size: 14px;}



